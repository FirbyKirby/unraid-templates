<?xml version="1.0"?>
<Container version="2">
  <Name>BabyBuddy</Name>
  <Repository>linuxserver/babybuddy</Repository>
  <Registry>https://hub.docker.com/r/linuxserver/babybuddy</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://bit.ly/2MwDPTV</Support>
  <Project>https://github.com/linuxserver/docker-babybuddy</Project>
  <Overview>Babybuddy is a buddy for babies! Helps caregivers track sleep, feedings, diaper changes, tummy time and more to learn about and predict baby's needs without (as much) guess work.</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:8000]/</WebUI>
  <TemplateURL/>
  <Icon>https://i.imgur.com/gwwZ2Yo.png</Icon>
  <ExtraParams>--restart unless-stopped</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1626948204</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>Babybuddy is a buddy for babies! Helps caregivers track sleep, feedings, diaper changes, tummy time and more to learn about and predict baby's needs without (as much) guess work.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8000</HostPort>
        <ContainerPort>8000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/babybuddy</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>Australia/Melbourne</Value>
      <Name>TZ</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Appdata" Target="/config" Default="/mnt/user/appdata/babybuddy" Mode="rw" Description="Container Path: /config" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/babybuddy</Config>
  <Config Name="Timezone" Target="TZ" Default="Australia/Melbourne" Mode="" Description="Timezone" Type="Variable" Display="always" Required="false" Mask="false">Australia/Melbourne</Config>
  <Config Name="WebUI" Target="8000" Default="8000" Mode="tcp" Description="Container Port: 8000" Type="Port" Display="always" Required="true" Mask="false">8000</Config>
</Container>